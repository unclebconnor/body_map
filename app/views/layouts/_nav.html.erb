<div class="nav-wrapper level-right level-item">
  <% if current_user %>
    <div class="level-item nav-item">
    	<a href="/user/<%=current_user.id%>"><img class="thumb" src="<%= current_user.img_url %>"></img></a>
    </div> 
    <div class="level-item nav-item">
		<%= link_to "Sign out", signout_path, id: "sign_out" %>
	</div>
    
  <% else %>
    <div class="level-item nav-item">
      <%= image_tag "googleIcon.png", class: "thumb" %>
    </div>
    <div class="level-item nav-item">
    	<%= link_to "Sign In", "/auth/google_oauth2", id: "sign_in" %>
    </div>
    
  <% end %>
</div>