<section class="section">	
	<div class="container is-fluid">
		<h1>Annotation#index</h1>
		<p>List of annotations for this map</p>
		<% map.annotations.each do |a| %>
			<div class="columns box">
				<div class="column"><%= a.title %></div>
				<div class="column"><%= a.description %></div>
				<div class="column"><%= a.link %></div>
			</div>
		<% end %>
	</div>
</section>
